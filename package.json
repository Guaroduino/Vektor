    "docs:gen": "node ./scripts/gen-lib-docs.js"
  },
  "devDependencies": {
    "typescript": "~5.9.3",
    "vite": "^7.1.7"
  },
  "dependencies": {
    "pixi.js": "^8.14.0"
  }
}
